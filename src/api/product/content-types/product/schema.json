{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "desc": {
      "type": "text"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal"
    },
    "isNew": {
      "type": "boolean",
      "default": false
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "products"
    },
    "sub_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::sub-category.sub-category",
      "inversedBy": "products"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "normal",
        "featured",
        "trending"
      ]
    },
    "sku": {
      "type": "string"
    },
    "stock_quantity": {
      "type": "integer"
    },
    "inventory_status": {
      "type": "enumeration",
      "enum": [
        "inStock",
        "outOfStock",
        "lowStock"
      ]
    },
    "low_stock_threshold": {
      "type": "integer"
    },
    "restock_date": {
      "type": "datetime"
    },
    "inventory_location": {
      "type": "string"
    },
    "color": {
      "type": "string"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "isSpecialProduct": {
      "type": "boolean",
      "default": false
    }
  }
}
